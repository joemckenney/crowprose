{
  "name": "@www/app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "exports": {
    "./template": {
      "types": "./dist/template/index.d.ts",
      "import": "./dist/template/template.js"
    },
    "./ssr": {
      "types": "./dist/server/index.d.ts",
      "import": "./dist/server/entry-server.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "pnpm run --stream --color \"/build:/\"",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build -c ./vite.server.config.ts --ssr src/entry-server.tsx --outDir dist/server",
    "build:template": "vite build -c ./vite.template.config.ts --ssr src/template.ts --outDir dist/template",
    "build:prerender": "vite build -c ./vite.prerender.config.ts",
    "build:ssg": "pnpm build:client && pnpm build:server && pnpm build:prerender && node dist/server/prerender.js",
    "format": "biome format --write ./src",
    "lint": "biome lint ./src",
    "preview": "vite preview"
  },
  "dependencies": {
    "@mdx-js/react": "3.1.0",
    "@vanilla-extract/css": "1.15.2",
    "posthog-js": "1.335.2",
    "prism-react-renderer": "2.4.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.23.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.0",
    "@mdx-js/rollup": "3.1.0",
    "@types/mdx": "2.0.13",
    "gray-matter": "4.0.3",
    "@types/node": "20.14.2",
    "@types/react": "18.2.66",
    "@types/react-dom": "18.2.22",
    "@vanilla-extract/vite-plugin": "4.0.10",
    "@vitejs/plugin-react": "4.2.1",
    "compression": "1.7.4",
    "cross-env": "7.0.3",
    "express": "4.19.2",
    "serve-static": "1.15.0",
    "typescript": "5.2.2",
    "remark-frontmatter": "5.0.0",
    "remark-mdx-frontmatter": "5.0.0",
    "vite": "5.2.0",
    "vite-plugin-dts": "3.9.1"
  }
}
